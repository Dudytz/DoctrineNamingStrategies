{
    "name": "eduardo-leggiero/doctrine-naming-strategies-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine naming strategies",
    "license": "MIT",
    "keywords": [ "doctrine", "naming", "strategy", "strategies", "plural", "underscore", "camelcase" ],
    "homepage": "http://github.com/EduardoLeggiero/DoctrineNamingStrategiesBundle",
    "authors": [{
        "name": "Eduardo Leggiero",
        "homepage": "http://leggiero.uk"
    }],
    "require": {
        "php": ">=5.3.3",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "autoload": {
        "psr-0": { "EduardoLeggiero\\DoctrineNamingStrategiesBundle": "" }
    },
    "target-dir": "EduardoLeggiero/DoctrineNamingStrategiesBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
